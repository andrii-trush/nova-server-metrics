{
    "name": "llaski/nova-server-metrics",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/nova": "^1.0|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Llaski\\NovaServerMetrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Llaski\\NovaServerMetrics\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
