{
    "name": "llaski/nova-server-metrics",
    "description": "A Laravel Nova card.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8||^9",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Llaski\\NovaServerMetrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Llaski\\NovaServerMetrics\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
